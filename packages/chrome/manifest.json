{
  "name": "iCursor",
  "description": "Move the cursor where you look.",
  "version": "1.2.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "gg"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "injection/injection.css"
      ],
      "js": [
        "injection/injection.js"
      ]
    }
  ],
  "action": {
    "default_title": "iCursor or something",
    "default_icon": {
      "32": "icons/icon.png",
      "72": "icons/icon.png",
      "128": "icons/icon.png",
      "512": "icons/icon.png"
    },
    "default_popup": "popup.html"
    
  },
  "icons": {
    "32": "icons/icon.png",
    "72": "icons/icon.png",
    "128": "icons/icon.png",
    "512": "icons/icon.png"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "nativeMessaging",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
}
